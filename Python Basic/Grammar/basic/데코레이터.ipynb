    "def decoarg(arg):\n",
    "    print(f\"decoarg: {arg}\")\n",
    "    def decofunc(func):\n",
    "        def wrapper(*args, **kwargs):\n",
    "            print(f\"wrapper: {arg}\")\n",
    "            decovalue = func(*args, **kwargs)\n",
    "            print(decovalue*5)\n",
    "        return wrapper\n",
    "    return decofunc"
   ]
  },
